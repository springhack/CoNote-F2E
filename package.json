{
  "name": "conote-f2e",
  "version": "1.0.0",
  "description": "CoNote F2E project",
  "main": "./lib/main.js",
  "scripts": {
    "test": "npm run build",
    "dev": "webpack-dev-server --devtool eval --progress --hot --colors --content-base build",
    "bs": "mkdir -p lib && babel src/common -d lib && babel src/server -d lib",
    "bc": "NODE_ENV=production webpack --progress --config webpack.production.config.js",
    "build": "npm run bc && npm run bs",
    "start": "NODE_SSR=true node lib/main.js"
  },
  "keywords": [
    "SpringHack",
    "CoNote"
  ],
  "author": "SpringHack",
  "license": "GPL-3.0",
  "babel": {
    "presets": [
      "react",
      "latest",
      "stage-0"
    ],
    "plugins": [
      "babel-plugin-transform-decorators-legacy",
      "react-hot-loader/babel"
    ]
  },
  "dependencies": {
    "babel-cli": "latest",
    "babel-core": "latest",
    "babel-loader": "latest",
    "babel-plugin-transform-decorators-legacy": "latest",
    "babel-plugin-transform-react-jsx": "latest",
    "babel-polyfill": "latest",
    "babel-preset-latest": "latest",
    "babel-preset-react": "latest",
    "babel-preset-stage-0": "latest",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "css-loader": "latest",
    "express": "latest",
    "express-session": "^1.14.2",
    "extract-text-webpack-plugin": "latest",
    "html-webpack-plugin": "latest",
    "less": "latest",
    "less-loader": "latest",
    "mobx": "latest",
    "mobx-react": "latest",
    "muicss": "latest",
    "multer": "^1.2.0",
    "open-browser-webpack-plugin": "^0.0.3",
    "postcss-loader": "^1.2.2",
    "react": "latest",
    "react-dom": "latest",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-router": "^3.0.2",
    "style-loader": "latest",
    "webpack": "latest",
    "webpack-dev-server": "^1.16.2"
  }
}
